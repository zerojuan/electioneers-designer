{
  "name": "ElectioneersDesigner",
  "version": "0.0.6",
  "description": "Designer tool for Electioneers",
  "repository": "https://github.com/zerojuan/electioneers-designer",
  "main": "index.html",
  "scripts": {
    "postinstall": "install-app-deps",
    "start": "npm install && electron ./app",
    "local": "gulp watch",
    "localelectron": "gulp copy-assets && electron ./app",
    "clean": "rimraf dist/",
    "clean:osx": "rimraf dist/ElectioneersDesigner-darwin-x64",
    "clean:win": "rimraf dist/win",
    "compile": "rimraf ./app/out && mkdir app/out && cp -r assets/** app/out",
    "compile:win": "rimraf app/out && mkdir app\\out && xcopy assets app\\out",
    "pack": "./node_modules/.bin/gulp build && npm run compile && build",
    "dist": "npm run compile && ./node_modules/.bin/gulp build && build --dist",
    "dist:win": "npm run compile:win && gulp build && build --dist",
    "test": "babel-tape-runner test/**/*.js",
    "test-ci": "babel-tape-runner test/**/*.js | tap-xunit"
  },
  "keywords": [
    "electioneers",
    "webkit",
    "designer"
  ],
  "engines": {
    "node": "6.0.0"
  },
  "build": {
    "app-bundle-id": "xyz.electioneers.electioneersdesigner",
    "app-category-type": "public.app-category.games",
    "osx": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    }
  },
  "author": "juliusthepious",
  "license": "ISC",
  "dependencies": {
    "async": "^1.5.1",
    "body-parser": "^1.14.2",
    "cors": "^2.7.1",
    "electron-is-dev": "^0.1.0",
    "express": "^4.13.3",
    "fs-extra": "^0.26.4",
    "lodash": "^4.17.19",
    "moniker": "^0.1.2",
    "multer": "^1.1.0",
    "nslog": "^3.0.0",
    "react-logger": "^1.0.0",
    "reselect": "^2.5.1"
  },
  "devDependencies": {
    "babel-core": "^6.3.26",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^6.2.0",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-2": "^6.3.13",
    "css-loader": "^3.0.0",
    "d3": "^3.5.14",
    "electron-builder": "^21.0.15",
    "electron-prebuilt": "^0.37.6",
    "es6-promise": "^3.0.2",
    "eslint": "^6.0.1",
    "eslint-plugin-react": "^3.14.0",
    "file-loader": "^0.8.5",
    "gulp": "^4.0.2",
    "gulp-less": "^4.0.1",
    "gulp-live-server": "0.0.31",
    "gulp-load-plugins": "^1.1.0",
    "gulp-util": "^3.0.7",
    "gulp-watch": "^5.0.1",
    "history": "^1.13.1",
    "isomorphic-fetch": "^2.2.0",
    "jscs": "^3.0.7",
    "jscs-loader": "^0.2.0",
    "less-loader": "^2.2.2",
    "map-stream": "0.0.6",
    "material-ui": "^0.14.4",
    "mock-fs": "^3.8.0",
    "moment": "^2.11.0",
    "moniker": "^0.1.2",
    "p2": "^0.7.1",
    "phaser": "^2.4.6",
    "react": "^0.14.7",
    "react-addons-update": "^0.14.7",
    "react-breadcrumbs": "^1.1.9",
    "react-dom": "^0.14.3",
    "react-dropzone": "^3.3.2",
    "react-hot-loader": "^1.3.0",
    "react-material-icons": "^1.0.1",
    "react-redux": "^4.0.6",
    "react-router": "^2.0.1",
    "react-tap-event-plugin": "^0.2.1",
    "react-time": "^4.0.0",
    "redux": "^3.0.5",
    "redux-logger": "^2.3.1",
    "redux-thunk": "^1.0.3",
    "script-loader": "^0.6.1",
    "style-loader": "^0.13.0",
    "supertest": "^4.0.2",
    "tap-xunit": "^2.4.1",
    "tape": "^4.5.1",
    "touch": "^1.0.0",
    "underscore": "^1.8.3",
    "webpack": "^4.35.3",
    "webpack-dev-server": "^3.7.2"
  }
}
